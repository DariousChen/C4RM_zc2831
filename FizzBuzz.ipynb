# Have this code run for any start and finish values

def FizzBuzz(start, finish):
    outlist = []

    for i in range(start, finish+1):

        if i % 3 == 0 and i % 5 == 0:
            outlist.append("fizzbuzz")
        elif i % 3 == 0:
            outlist.append("fizz")
        elif i % 5 == 0:
            outlist.append("buzz")
        else:
            outlist.append(i)
    return(outlist)

start = 1
finish = 15
x = FizzBuzz(start,finish)
print(x)
